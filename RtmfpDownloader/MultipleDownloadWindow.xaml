<Window x:Class="RtmfpDownloader.MultipleDownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RtmfpDownloader"
        mc:Ignorable="d"
        Title="批量下载" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" TextWrapping="Wrap" Text="" VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" Margin="0,0,0,30" AcceptsReturn="True"/>
        <Button x:Name="button" Content="开始下载" HorizontalAlignment="Right" Margin="0,0,0,1" VerticalAlignment="Bottom" Width="75" Click="StartDownload"/>

    </Grid>
</Window>
